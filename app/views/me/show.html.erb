<div class="container" style="width:2000px">
<% if current_user %>
<h1 >Your Details</h1>
<table class=""table table-striped"">
	<% current_user.attributes.each do |k, v| %>

		<b><%= k %>:</b> <%= v %> <br>
	<% end %><br><br>
<h3>You have signed with <strong class="fa fa-google" style="font-size:28px;color:blue">oogle</strong></h3>

<% end %>
</table>


<h1><%= link_to "LogOut",sessions_destroy_path,:class => 'btn btn-large btn-danger' %></h1>
</div>